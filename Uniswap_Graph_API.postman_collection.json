		{
			"name": "Fetch Most Liquid Pools",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "graphql",
					"graphql": {
						"query": "{\n  pools(\n    first: 100,\n    orderBy: liquidity,\n    orderDirection: desc,\n    where: {\n      liquidity_gt: \"0\"\n    }\n  ) {\n    id\n    token0 {\n      id\n      symbol\n      name\n    }\n    token1 {\n      id\n      symbol\n      name\n    }\n    liquidity\n    volumeUSD\n    feeTier\n    sqrtPrice\n    totalValueLockedUSD\n  }\n}",
						"variables": ""
					}
				},
				"url": {
					"raw": "https://gateway-arbitrum.network.thegraph.com/api/{{GRAPH_API_KEY}}/subgraphs/id/5zvR82QoaXYFyDEKLZ9t6v9adgnptxYpKpSbxtgVENFV",
					"protocol": "https",
					"host": [
						"gateway-arbitrum",
						"network",
						"thegraph",
						"com"
					],
					"path": [
						"api",
						"{{GRAPH_API_KEY}}",
						"subgraphs",
						"id",
						"5zvR82QoaXYFyDEKLZ9t6v9adgnptxYpKpSbxtgVENFV"
					]
				},
